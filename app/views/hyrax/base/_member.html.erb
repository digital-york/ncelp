<tr class="<%= dom_class(member) %> attributes">
  <td class="thumbnail">
    <%= render_ncelp_thumbnail_tag member, contextual_path(member, @presenter), { alt: "Download file " + member.link_name }, { title: "Download file " + member.link_name } %>
  </td>
  <td class="attribute attribute-filename ensure-wrapped"><%= link_to(member.link_name, contextual_path(member, @presenter), title: "Download file " + member.link_name) %></td>
  <td class="attribute attribute-date_uploaded"><%= member.try(:date_uploaded) %></td>
  <td class="attribute attribute-permission"><%= member.permission_badge %></td>
  <td>
    <%= render 'actions', member: member %>
  </td>
</tr>
