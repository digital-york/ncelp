<% presenter.title.each_with_index do |title, index| %>
  <div class="row">
    <div class="col-sm-6">
      <% if index == 0 %>
        <h2>Resource details
        </h2>
      <% else %>
        <h2><%= title %></h2>
      <% end %>
    </div>
    <div class="col-sm-6 text-right">
      <% if index == 0 %>
        <%= render "show_ncelp_actions", presenter: presenter %>
      <% end %>
    </div>
  </div>
<% end %>
